# GitHub API Configuration
# Get your Personal Access Token from: https://github.com/settings/tokens
GITHUB_TOKEN=ghp_your_personal_access_token_here

# Analysis Configuration
# FDV Ratio thresholds (default: warn if < 40%)
FDV_RATIO_WARNING_THRESHOLD=0.4
FDV_RATIO_TARGET_MIN=0.45
FDV_RATIO_TARGET_MAX=0.50

# Developer Activity Configuration
# Number of days to look back for commit activity
DEV_ACTIVITY_LOOKBACK_DAYS=30

# Operational Configuration
LOG_LEVEL=INFO
HTTP_TIMEOUT=30
MAX_RETRIES=3
